function editor(snake)
	return{
		index = 0,
		update=function(self)
			while true do
				if btnp(🅾️) then
					deli(snake.body,self.index)
				elseif btnp(❎) then
					if self.index == 0 and #snake.body>0 then
						snake:reset(1,0,1)
						scene = player(snake)
						return
					end
					add(snake.body,nop(),self.index)
					self.index+=1
				elseif btnp(⬆️) then
					self.index-=1
				elseif btnp(⬇️) then
					self.index+=1
				elseif btnp(⬅️) then
					add(snake.body,left(),self.index)
					self.index+=1
				elseif btnp(➡️) then
					add(snake.body,right(),self.index)
					self.index+=1
				end
				if 0>self.index then self.index=min(15,#snake.body+1)
				elseif min(15,#snake.body+1)<self.index then self.index=0 end
				yield()
			end
		end,
		draw=function(self)
			cls()
			spr(0,8,0)
			for i=1,#snake.body do
				spr(1,8,i*8)
				print(i,0,i*8+2)
				spr(snake.body[i].spr,8,i*8)
				print(snake.body[i].desc,24,i*8+2)
			end
			spr(6,8,#snake.body*8+8)
			spr(2,8,self.index*8)
			if self.index==0 then
			 if #snake.body == 0 then
			 	print("snake should have\nat least 1 body",24,1)
			 else
					print("press ❎ to start",24,1)
				end
			end
			print("🅾️:delete\n❎:add forward\n⬅️:add left\n➡️:add right",72,24)
			rectfill(79,79,100,100,0)
			for i=0,6 do
			 for j=0,6 do
			 	local tile = mget(i,j)
			 	local col=0
			 	if fget(tile,1) then
			 		if fget(tile,2) then
			 			col=9
			 		elseif fget(tile,3) then
			 			col=12
			 		else
			 			col=13
			 		end
			 	elseif fget(tile,0) then
			 		col=1
			 	end
			 	rect(80+i*3,80+j*3,80+i*3+1,80+j*3+1,col)
			 end
			end
			print("\f6map preview\n\f9enter, \fcexit",79,67)
			color(6)
		end
	}
end

function player(snake)
	return{
		dead = false,
		update=function(self)
			local t = 0
			local tile = mget(snake.headx,snake.heady)
			while fget(tile,0) do
			 if t > 10 then
					snake:proceed()
					t=0
				else
				 t+=1
				end
				tile = mget(snake.headx,snake.heady)
				yield()
			end
			self.dead=true
			while true do
				if btnp(❎) then break end
				yield()
			end
			scene=editor(snake)
			return
		end,
		draw=function(self)
			cls()
			if self.dead then print("\#2\f9press ❎ to go back to editor\#0\f6",2,2) end
			camera(-36,-36)
			map(0,0,0,0,7,7,1)
			snake:draw()
			palt(0x0400)
			map(0,0,0,0,7,7,2)
			palt()
			camera(0,0)
		end
	}
end
