function snake(x,y,dir)
	local direction = {
		[0]={0,-1},
		{-1,0},
		{0,1},
		{1,0}
	}
	return {
		headx = x,
		heady = y,
		body = {},
		bodypos = {direction[dir]},
		index = 1,
		rotate = dir,
		reset = function(self,x,y,dir)
		 self.headx,self.heady,self.rotate,self.index=x-direction[dir][1],y-direction[dir][2],dir,1
			self.bodypos={direction[dir],direction[dir]}
		end,
		addpart=function(self,part,index)
			index=index or #self.body+1
			add(self.body,part,index)
		end,
		removepart=function(self,index)
			index=index or #self.body
			self.body[index].remove(self,index)
		end,
		proceed = function(self)
			if(self.index>#self.body) self.index = 1
			self.body[self.index].execute(self)
			self.rotate%=4
			self.headx-=direction[self.rotate][1]
			self.heady-=direction[self.rotate][2]
			add(self.bodypos,direction[self.rotate],1)
			while(#self.bodypos>#self.body+1) do deli(self.bodypos,#self.bodypos) end
		end,
		draw = function(self)
			local bx,by=self.headx,self.heady
			spr(0,bx*8,by*8)
			for i=1,#self.bodypos-1 do
				bx+=self.bodypos[i][1]
				by+=self.bodypos[i][2]
				spr(1,bx*8,by*8)
				spr(self.body[i].bspr,bx*8,by*8)
				spr(self.body[i].fspr,bx*8,by*8)
				if(i==self.index-1) spr(2,bx*8,by*8)
			end
		end
	}
end
