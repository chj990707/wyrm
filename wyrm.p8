function _init()
	scene = editor(snake(0,0,1))
	curco = cocreate(scene.update)
end

local i = 0
function _update()
	if costatus(curco) ~= "dead" then
		coresume(curco, scene)
	else
		curco = cocreate(scene.update)
	end
end

function _draw()
	scene:draw()
end

