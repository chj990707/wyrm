function nop()
	return {
		desc = "go forward",
		spr = 3,
		execute = function(snake)
			snake.index+=1
		end
	}
end

function left()
	local base=nop()
	local base_exec=base.execute
	base.execute = function(snake)
		base_exec(snake)
		snake.rotate-=1
	end
	base.spr=4
	base.desc="turn left"
	return base
end

function right()
	local base=nop()
	local base_exec=base.execute
	base.execute = function(snake)
		base_exec(snake)
		snake.rotate+=1
	end
	base.spr=5
	base.desc="turn right"
	return base
end

