function editor(snake)
	local funcs={[0]=nop(),forward(),left(),right()}
	local subwindow = {
		active=false,
		body_i=1,
		update = function(self)
			self.active=true
			self.body_i=1
			yield()
			while not btnp(‚ùé) do
				if btnp(‚¨ÜÔ∏è) then
					self.body_i-=1
				elseif btnp(‚¨áÔ∏è) then
					self.body_i+=1
				end
				self.body_i%=#funcs+1
				yield()
			end
			self.active=false
		end
	}
	return{
		index = 0,
		update=function(self)
			while true do
				if btnp(üÖæÔ∏è) or btnp(‚ùé) then
					if self.index == 0 then
						if #snake.body>0 then
							snake:reset(1,6,2)
							scene=player(snake)
							return
						end
					else
						if btnp(‚ùé) and #snake.body>=self.index then snake:removepart(self.index) end
						snake:addpart(funcs[1],self.index)
						repeat
							yield()
							subwindow:update()
						until not subwindow.active
					end
					snake:removepart(self.index)
					if subwindow.body_i~=0 then
						snake:addpart(funcs[subwindow.body_i],self.index)
					end
				elseif btnp(‚¨ÜÔ∏è) then
					self.index-=1
				elseif btnp(‚¨áÔ∏è) then
					self.index+=1
				end
				if 0>self.index then self.index=min(15,#snake.body+1)
				elseif min(15,#snake.body+1)<self.index then self.index=0 end
				yield()
			end
		end,
		draw=function(self)
			cls()
			spr(0,8,0)
			for i=1,#snake.body do
				print(i,0,i*8+2)
				if subwindow.active and i==self.index then
					spr(funcs[subwindow.body_i].bspr,8,i*8)
					spr(funcs[subwindow.body_i].fspr,8,i*8)
				else
					spr(snake.body[i].bspr,8,i*8)
					spr(snake.body[i].fspr,8,i*8)
					print(snake.body[i].desc,26,i*8+2)
				end
			end
			spr(3,8,#snake.body*8+8)
			spr(31,16,self.index*8)
			if self.index==0 then
			 if #snake.body == 0 then
			 	print("snake should have\nat least 1 body",26,1)
			 else
					print("üÖæÔ∏è/‚ùé:start",26,1)
				end
			else
				print("üÖæÔ∏è:add, ‚ùé:edit",26,1)
			end
			if subwindow.active then
				rectfill(26,self.index*8-6,80,self.index*8+14,1)
				rect(26,self.index*8-6,80,self.index*8+15,6)
				print("‚¨ÜÔ∏è"..funcs[(subwindow.body_i-1)%(#funcs+1)].desc,28,self.index*8-4,5)
				print("‚ùé"..funcs[subwindow.body_i].desc,28,self.index*8+2,6)
				print("‚¨áÔ∏è"..funcs[(subwindow.body_i+1)%(#funcs+1)].desc,28,self.index*8+8,5)
			end
			rectfill(79,79,100,100,0)
			for i=0,6 do
			 for j=0,6 do
			 	local tile = mget(i,j)
			 	local col=0
			 	if fget(tile,1) then
			 		if fget(tile,2) then
			 			col=9
			 		elseif fget(tile,3) then
			 			col=12
			 		else
			 			col=13
			 		end
			 	elseif fget(tile,0) then
			 		col=1
			 	end
			 	rect(80+i*3,80+j*3,80+i*3+1,80+j*3+1,col)
			 end
			end
			print("\f6map preview\n\f9enter, \fcexit",79,67)
			color(6)
		end
	}
end

function player(snake)
	return{
		dead = false,
		update=function(self)
			local t = 0
			local tile = mget(snake.headx,snake.heady)
			while fget(tile,0) do
			 if t > 10 then
					snake:proceed()
					t=0
				else
				 t+=1
				end
				tile = mget(snake.headx,snake.heady)
				yield()
			end
			self.dead=true
			while true do
				if btnp(‚ùé) then break end
				yield()
			end
			scene=editor(snake)
			return
		end,
		draw=function(self)
			cls()
			if self.dead then print("\#2\f9press ‚ùé to go back to editor\#0\f6",2,2) end
			camera(-36,-36)
			map(0,0,0,0,7,7,1)
			snake:draw()
			palt(0x0002)
			map(0,0,0,0,7,7,2)
			palt()
			camera(0,0)
		end
	}
end
