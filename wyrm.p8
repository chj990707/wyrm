function nop()
	return {
		desc = "empty",
		bspr = 3,
		fspr = 3,
		execute = function(snake)
			snake.index+=1
		end,
		remove = function(snake, i)
			deli(snake.body, i)
		end
	}
end

function forward()
	local base=nop()
	base.desc,base.bspr,base.fspr = "go forward",1,4
	return base
end

function left()
	local base=forward()
	local base_exec=base.execute
	base.execute = function(snake)
		base_exec(snake)
		snake.rotate+=1
	end
	base.fspr=5
	base.desc="turn left"
	return base
end

function right()
	local base=forward()
	local base_exec=base.execute
	base.execute = function(snake)
		base_exec(snake)
		snake.rotate-=1
	end
	base.fspr=6
	base.desc="turn right"
	return base
end

